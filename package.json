        "README.md",
        "bin"
    ],
    "dependencies": {
        "axios": "^0.21.1",
        "chalk": "^4.1.1",
        "commander": "^7.2.0",
        "crypto": "^1.0.1",
        "enquirer": "^2.3.6",
        "figures": "^3.2.0",
        "fs-extra": "^10.0.0",
        "json-schema-to-typescript": "^10.1.4",
        "openapi3-ts": "^2.0.2",
        "prettier": "^2.3.0",
        "project-build-ci": "^1.0.12",
        "qs": "^6.10.5",
        "squirrelly": "^8.0.8",
        "swagger2openapi": "^7.0.8"
    },
    "devDependencies": {
        "@types/fs-extra": "^9.0.11",
